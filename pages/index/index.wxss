@charset "UTF-8";
.yg_index swiper image {
  width: 100%;
}

.yg_index .cate {
  display: flex;
}

.yg_index .cate navigator {
  flex: 1;
  padding: 20rpx;
}

.yg_index .cate navigator image {
  width: 100%;
}

.yg_index .floor .group .title {
  padding: 10rpx 0;
}

.yg_index .floor .group .title image {
  width: 100%;
}

.yg_index .floor .group .list {
  overflow: hidden;
}

.yg_index .floor .group .list navigator {
  float: left;
  width: 33.3%;
}

.yg_index .floor .group .list navigator:nth-last-child(-n+4) image {
  /*
                            原图232 * 386
                            232 / 386 = 33.33vw / ?height
                            第一张 ?height = 33.33vw * 386 / 232
                            */
  height: 27.72711vw;
  border-left: 10rpx solid #fff;
}

.yg_index .floor .group .list navigator:nth-child(2), .yg_index .floor .group .list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}

.yg_index .floor .group .list navigator image {
  width: 100%;
  height: 100%;
}
